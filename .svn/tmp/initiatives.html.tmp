  <script>
  	var $loc = window.location.href; // returns the full URL
  	if ($loc.indexOf("#") > -1) {
		var $target = $loc.substring($loc.lastIndexOf("#"), $loc.length);
		var $activeID = document.getElementById($target);
		$($target).addClass('active');
	}

	var $parent = $('.initiative.i-row');
	var $anchor = $('.initiative.i-row .anchor');
	$anchor.on("click", function(e){
		e.preventDefault(); // prevents hash snap from occuring, so smooth scroll occurs
		console.log("click");
		$parent.removeClass('active');
		if ($(this).closest($parent).hasClass('active') !== true) {
			//console.log($(this).closest($parent));
			console.log("the closest parent is not active");
		   $(this).closest($parent).addClass('active');
		   console.log("the closest parent is now active");
		} else {
			console.log("the closest parent is already active");
			//$(this).closest($parent).removeClass('active');
			$parent.removeClass('active');
		}
	});

